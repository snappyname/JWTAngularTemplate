{
	"name": "template-web-auth",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"lint": "ng lint",
		"deploy": "npm run lint && npm run build",
		"test": "ng test"
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 4,
		"printWidth": 120,
		"singleQuote": true,
		"overrides": [
			{
				"files": "*.html",
				"options": {
					"parser": "angular"
				}
			},
			{
				"files": [
					"*.css",
					"*.scss",
					"*.sass"
				],
				"options": {
					"parser": "css"
				}
			}
		]
	},
	"private": true,
	"dependencies": {
		"@angular/common": "^20.3.0",
		"@angular/compiler": "^20.3.0",
		"@angular/core": "^20.3.0",
		"@angular/forms": "^20.3.0",
		"@angular/platform-browser": "^20.3.0",
		"@angular/router": "^20.3.0",
		"@microsoft/signalr": "^10.0.0",
		"@ngxs/devtools-plugin": "20.1.0",
		"@ngxs/form-plugin": "20.1.0",
		"@ngxs/hmr-plugin": "20.1.0",
		"@ngxs/logger-plugin": "20.1.0",
		"@ngxs/router-plugin": "20.1.0",
		"@ngxs/storage-plugin": "20.1.0",
		"@ngxs/store": "^20.1.0",
		"@ngxs/websocket-plugin": "20.1.0",
		"jwt-decode": "^4.0.0",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.15.0"
	},
	"devDependencies": {
		"@angular/build": "^20.3.13",
		"@angular/cli": "^20.3.13",
		"@angular/compiler-cli": "^20.3.0",
		"@eslint/js": "^9.39.2",
		"@types/jasmine": "~5.1.0",
		"angular-eslint": "21.0.1",
		"eslint": "^9.39.1",
		"globals": "^16.5.0",
		"jasmine-core": "~5.9.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"prettier": "^3.7.4",
		"typescript": "~5.9.2",
		"typescript-eslint": "8.46.4"
	}
}
